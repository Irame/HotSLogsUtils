{
	"manifest_version": 2,

	"name": "HotSLogsUtils",
	"description": "Small Chrome extension to add simple funktionality to HotSLogs",
	"version": "1.0",

	"permissions": [
		"https://www.hotslogs.com/*"
	],
	"content_scripts": [
		{
			"matches": ["https://www.hotslogs.com/Player/MatchPreview*"],
			"js": ["jquery-3.1.1.min.js", "jquery-regex.js", "linkHero.js"],
			"run_at": "document_end"
		}
	]
}
